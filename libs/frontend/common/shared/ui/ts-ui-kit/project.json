{
  "name": "frontend-common-shared-ui-ts-ui-kit",
  "$schema": "../../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/common/shared/ui/ts-ui-kit/src",
  "prefix": "lib",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/frontend/common/shared/ui/ts-ui-kit/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/frontend/common/shared/ui/ts-ui-kit/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/frontend/common/shared/ui/ts-ui-kit/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/frontend/common/shared/ui/ts-ui-kit/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/frontend/common/shared/ui/ts-ui-kit/**/*.ts",
          "libs/frontend/common/shared/ui/ts-ui-kit/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/frontend/common/shared/ui/ts-ui-kit/.storybook",
        "browserTarget": "frontend-common-shared-ui-ts-ui-kit:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/frontend-common-shared-ui-ts-ui-kit",
        "configDir": "libs/frontend/common/shared/ui/ts-ui-kit/.storybook",
        "browserTarget": "frontend-common-shared-ui-ts-ui-kit:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "frontend-common-shared-ui-ts-ui-kit:build-storybook",
        "staticFilePath": "dist/storybook/frontend-common-shared-ui-ts-ui-kit"
      },
      "configurations": {
        "ci": {
          "buildTarget": "frontend-common-shared-ui-ts-ui-kit:build-storybook:ci"
        }
      }
    }
  }
}
